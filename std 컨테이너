#include <iostream>
#include <vector>

int main() {
    std::vector<int> numbers = {5, 2, 9, 1, 7};

    std::cout << "원래 벡터: ";
    for (int n : numbers) std::cout << n << " ";
    std::cout << std::endl;

    numbers.push_back(4);

    std::cout << "벡터 크기: " << numbers.size() << std::endl;

    return 0;
}
